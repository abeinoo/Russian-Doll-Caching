<%= Rails.application.config.cache_store %>
<p id="notice"><%= notice %></p>

<% cache ["lists-index" , @lists.map(&:updated_at).max.utc.to_s(:usec)] do %>

<h1>Lists</h1>

<%= render partial: 'lists/list', collection: @lists, cached: true %>

<br>

<%= link_to 'New List', new_list_path %>

<%= will_paginate @lists %>
<% end %>